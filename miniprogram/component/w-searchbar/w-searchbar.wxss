/* component/searchbar/searchbar.wxss */
.search{
  padding: 20rpx;
}

.searchbar{
  background-color: #f2f2f2;
  border-radius:50rpx;
  width: 100%;
  display: flex;
}

.search-navigator{
  flex-grow: 1;
  height: 70rpx;
  background-image: url("data:image/png;base64,");
  background-position: 2% 50%;
  background-repeat:no-repeat;
  background-size: 8%;
  color: #aaaaaa;
  font-size: 30rpx;
  line-height: 70rpx;
  text-align: center;
}


.scanimage{
  padding: 14rpx 20rpx;
  width: 42rpx;
  height: 42rpx;
}


.searchinput{
  height: 70rpx;
  margin-left: 20rpx;
  flex-grow: 1;
  color: #aaaaaa;
  font-size: 30rpx;
  line-height: 70rpx;
}

